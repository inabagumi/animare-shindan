{
  "author": "Haneru Developers",
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "date-fns": "^2.0.0-beta.3",
    "gatsby": "^2.13.45",
    "gatsby-plugin-emotion": "^4.1.2",
    "gatsby-plugin-google-analytics": "^2.1.6",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-nprogress": "^2.1.2",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-remove-trailing-slashes": "^2.1.2",
    "gatsby-plugin-sitemap": "^2.2.5",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.8",
    "gatsby-transformer-json": "^2.2.2",
    "gatsby-transformer-remark": "^2.6.10",
    "gatsby-transformer-yaml": "^2.2.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "description": "Shindan by AniMare",
  "devDependencies": {
    "@inabagumi/prettier-config": "^1.0.0",
    "@types/react": "^16.8.24",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "prettier": "^1.18.2",
    "typescript": "~3.5.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier/@typescript-eslint",
      "prettier/react"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "rules": {
          "@typescript-eslint/no-var-requires": "off"
        }
      }
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react/prop-types": "off",
      "react-hooks/exhaustive-deps": "error",
      "react-hooks/rules-of-hooks": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "homepage": "https://shindan.animare.cafe/",
  "license": "MIT",
  "name": "animare-shindan",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/inabagumi/animare-shindan.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write \"./**/*.{js,md,ts,tsx,yaml}\"",
    "lint": "eslint --ext .js,.ts,.tsx  --ignore-path .gitignore .",
    "now-build": "${npm_execpath} run build"
  },
  "version": "1.3.0"
}
